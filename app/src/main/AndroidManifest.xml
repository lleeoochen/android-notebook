<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.wei_tung.mynotebook"
          android:versionCode="1"
          android:versionName="Version 1">

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        android:label="Notebook"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.wei_tung.mynotebook.MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.wei_tung.mynotebook.EachNote">
        </activity>
    </application>

</manifest>